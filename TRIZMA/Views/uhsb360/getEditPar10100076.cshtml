    <script>
        $(document).on('click', '.omdvrf', function () {
            var userId = @ViewBag.userIds;
            var userNm = '@ViewBag.userNm';
            var id = $(this).attr('id').toString();
            var objid1 = id.slice(2, 12);
            var smid = "#sm" + objid1;
            var cmid = "#cm" + objid1;
            var dvid = "#dv" + objid1;
            var dvic = "#dv" + objid1;
            var vrf = "#vr" + objid1;
            var vrid = "#vr" + id;
            var vrf1 = $(vrf).is(":checked");
            if (vrf1 == true) {
                $(smid).val(userId).html(userNm);
                $(vrf).prop("checked", true);
                var tzoffset = (new Date()).getTimezoneOffset() * 60000;
                var d = (new Date(Date.now() - tzoffset));
                var eddt = d.toISOString(); // ISO "2013-12-08T17:55:38.130Z"
                var yr1 = eddt.slice(0, 4);
                var mn1 = eddt.slice(5, 7);
                var dy1 = eddt.slice(8, 10);
                var tm1 = eddt.slice(11, 16);
                var eddf = mn1 + "/" + dy1 + "/" + yr1 + " " + tm1;
                $(cmid).val(eddt).html(eddf);
                $(dvid).show();
                $(dvic).show();
            } else if (vrf1 == false) {
                $(dvid).hide();
                $(dvic).hide();
                $(smid).val(0);
                $(vrf).prop("checked", false);
                $(cmid).val(null);
            }
            setTopics();
        });
    </script>
