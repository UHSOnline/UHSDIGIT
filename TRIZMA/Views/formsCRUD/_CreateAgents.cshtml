                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.agentName, new { htmlAttributes = new { @class = "form-control", @style = "padding-left:10px; padding-right:10px; height: 34px; min-width: 220px; max-width: 220px; border-color: lightgray; border-style:solid; border-width:thin;" } })
                                    @*@Html.ValidationMessageFor(model => model.agentName, "", new { @class = "text-danger" })*@
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.UserName, "User Name:", htmlAttributes: new { @class = "control-label col-md-2", @style = "font-size:smaller" })
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.UserName, new { htmlAttributes = new { @class = "form-control", @style = "padding-left:10px; padding-right:10px; height: 34px; min-width: 220px; max-width: 220px; border-color: lightgray; border-style:solid; border-width:thin;" } })
