                                    @*@Html.ValidationMessageFor(model => model.editedDT, "", new { @class = "text-danger" })*@
                                </div>
                            </div>

                            @Html.HiddenFor(model => model.taskOrderID, new { Value = 1 })
                            @Html.HiddenFor(model => model.userID)
                            @Html.HiddenFor(model => model.createdByUserID, new { Value = ViewBag.currentUserLoginID101 })

                            <div class="form-group">
                                @Html.Label("*", htmlAttributes: new { @class = "control-label col-md-2", @style = "color:white" })
                                <div class="col-md-10">
                                    <button type="submit" class="btn btn-success">Create</button>
                                </div>
                            </div>
                            
                         

                        }
                        <br />
                        <div>
                            | @Html.ActionLink("Back to List", "Index")
                        </div>
                        <div class="ln_solid"></div>

