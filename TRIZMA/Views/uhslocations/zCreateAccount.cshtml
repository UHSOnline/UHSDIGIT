    <script type="text/javascript">
        $(".btnSave02").on('click', function (e) {
            e.preventDefault();
            postDoc();
        });
        function postDoc() {
            var IDc = @ViewBag.IDc;
            var wchk = $("#wchka").val();
            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: { IDc: IDc, actpid: 1, wchk: wchk},
                url: '/uhslocations/SaveA360',
                dataType: "JSON",
                success: function (conf) {
                    if(conf == 1){
                        window.location = "/uhslocations/IndexA360?projectID=6&taskOrderID=63&Int1=1";
                    }
                    else {
                        alert("Failed!");
                    }
                }
            });
        }
    </script>

