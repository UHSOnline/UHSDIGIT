    <script type="text/javascript">
        // Get the input field
        var input = document.getElementById("txtArea");
        // Execute a function when the user releases a key on the keyboard
        input.addEventListener("keyup", function (event) {
            // Number 13 is the "Enter" key on the keyboard
            if (event.keyCode === 13) {
                // Cancel the default action, if needed
                event.preventDefault();
                // Trigger the button element with a click
                document.getElementById("sndbtn").click();
            }
        });

        $("#sndbtn").on('click', function () {
            var sID = $("#sID").val();
            if (sID == null || sID == "") {
                var ut = @ViewBag.userTypeID;
                if (ut == 2) {
                    sendData(2);
                } else {
                    var sM = $("#selMod").val();
                    var sD = $("#selDiv").val();
                    var sL = $("#selLoc").val();
                    var sU = $("#selUsr").val();
                    var mT = $("#txtArea").val().replace(/(\r\n\t|\n|\r\t)/gm, " ");
                    if (sM == 0) {
                        alert("Please select module!");
                        return;
                    }
                    if (sD == 0) {
                        alert("Please select division!");
                        return;
                    }
                    if (sL == 0) {
                        alert("Please select location!");
                        return;
                    }
                    if (sU == 0) {
                        alert("Please select user!");
                        return;
                    } if (mT == null || mT == "") {
                        alert("Please write message!");
                        return;
                    }
                    sendData(0);
                }
                
            } else {
                var tzoffset = (new Date()).getTimezoneOffset() * 60000;
                var d = (new Date(Date.now() - tzoffset));
                var b = d.toISOString(); // ISO "2013-12-08T17:55:38.130Z"
                var c = @ViewBag.userID;
                var a = "#btr_" + sID;
                btsFun(a, sID, 3);
            }
        });
        function sendData(gU) {
            var tzoffset = (new Date()).getTimezoneOffset() * 60000;
            var d = (new Date(Date.now() - tzoffset));
            var b = d.toISOString(); // ISO "2013-12-08T17:55:38.130Z"
            var c = @ViewBag.userID;
            var a = b.slice(0, 4) + b.slice(5, 7) + b.slice(8, 10) + b.slice(11, 13) + b.slice(14, 16) + b.slice(17, 19) + b.slice(20, 23) + "T" + c + "U";
            var nul = null;
            var docmid = $("#selMod").val();
            var divsid = $("#selDiv").val();
            var distid = $("#selLoc").val();
            var acctid = $("#selLoc").val();
            var userid = $("#selUsr").val();
            var ispubl = [];
            if (gU == 2 && userid == 0) {
                ispubl = true;
            } else {
                ispubl = false;
            }
            var isrddt = nul;
            var isrodt = nul;
            var msgtxt = $("#txtArea").val().replace(/(\r\n\t|\n|\r\t)/gm, " ");
            var data = {
                  ID: a
                , IDT: nul
                , IDK: nul
                , projid: 16
                , tskoid: 67
                , dcgrid: 9010
                , dctpid: 9010001
                , tspdid: 1
                , distid: distid
                , acctid: acctid
                , userid: userid
                , dimgid: 0
                , docmid: docmid
                , ispubl: ispubl
                , isrepl: false
                , isrere: false
                , isread: false
                , islike: false
                , isrepo: false
                , isdlms: false
                , isrddt: nul
                , isrodt: nul
                , msgtxt: msgtxt
                , chck: false
                , chckid: userid
                , chckdt: nul
                , crdt: b
                , eddt: b
                , crusid: c
                , edusid: c
                , divsid: divsid
            };
            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                data: JSON.stringify(data),
                url: '/uhsCH/save9010001',
                dataType: "JSON",
                success: function (msgs) {
                    if (msgs.ispubl == true) {
                        return;
                    } else {
                        readData(msgs);
                        discard();
                    }
                    
                }
            });
        };
        function readData(msgs) {
            var userid = @ViewBag.userID;
            //console.log(msgs);
            $.each(msgs, function (ID, msgs) {
                var msg = [];
                var acctnm = [];
                var islike = [];
                var isread = [];
                var isrepo = [];
                var avatar = [];
                if (msgs.docmid == 1010003) {
                    avatar = "Visus.png' title='VWP Standards'";
                } else if (msgs.docmid == 1010006) {
                    avatar = "A360s.png' title='Asset360'";
                } else if (msgs.docmid == 1010007) {
                    avatar = "B360s.png' title='BioMed360'";
                } else if (msgs.docmid == 1010008) {
                    avatar = "Dists.png' title='District'";
                } else if (msgs.docmid == 1010009) {
                    avatar = "Surgs.png' title='Surgical Services'";
                } else if (msgs.docmid == 1010010) {
                    avatar = "Coeps.png' title='COE Ops'";
                } else if (msgs.docmid == 6010001) {
                    avatar = "Mapps.png' title='Inventory Mapping'";
                }
                if (msgs.acctnm != null) {
                    acctnm = " > " + msgs.acctnm;
                }
                if (msgs.ispubl == true) {

                }
                else {
                    if (msgs.isread == true) {
                        isread = "";
                    } else {
                        isread = "<a class='btn btn-xs lbl-default' id='bts_" + msgs.ID + "' data-id=\"" + msgs.isread + "\" onclick='btsFun(\"#bts_" + msgs.ID + "\",\"" + msgs.ID + "\",1)'><i class='fa fa-eye'></i> Seen </a>";
                    }
                    var lik = [];
                    if (msgs.islike == true) {
                        lik = "style='color: orange'";
                    } else {
                        lik = "";
                    }
                    islike = "<a class='btn btn-xs lbl-default' id='btl_" + msgs.ID + "' data-id=\"" + msgs.islike + "\" onclick='btsFun(\"#btl_" + msgs.ID + "\",\"" + msgs.ID + "\",2)' " + lik + "><i class='fa fa-thumbs-up'></i> Like </a>";
                    if (msgs.isrepo == true) {
                        isrepo = "<a class='btn btn-xs lbl-default' style='color: orange'><i class='fa fa-bullhorn'></i> Reported! </a>";

                    } else {
                        isrepo = "<a class='btn btn-xs lbl-default' id='btp_" + msgs.ID + "' data-id=\"" + msgs.isrepo + "\"  onclick='btsFun(\"#btp_" + msgs.ID + "\",\"" + msgs.ID + "\",4)'><i class='fa fa-bullhorn'></i> Report </a>";
                    }
                    if (msgs.isrepl == true) {
                        var col = [];
                        var dirct = [];
                        var side = "left col-lg-8";
                        dirct = "from: " + msgs.crusnm;
                        if (userid != msgs.crusid) {
                            col = "hotpink";
                        } else {
                            col = "forestgreen";
                            isread = "";
                            islike = "";
                            isrepo = "";
                        }
                        if (userid != msgs.chckid) {
                            side = "right col-lg-8";
                        }
                        var inst = "#dld_" + msgs.IDT;

                        msg = "<div class='pull-" + side + "' id='dlm_" + msgs.ID + "'><div class='chat-message'><div class='profile-hdtc'></div><div class='message' style='background-color:white; border:solid; border-width:thin; border-color:antiquewhite'><span style='color:" + col + "; font-size:12px'> > reply " + dirct + "</span><span class='message-date' style='font-size:12px; color:gray'> " + msgs.viewdt + "<a href='#' title='Delete for myself?' style='padding-left:12px' onclick='btsDel(\"" + msgs.ID + "\",2)'><i class='fa fa-times-circle-o' style='transform:scale(1.6)'></i></a></span><br><span class='message-content'>" + msgs.msgtxt + "</span><div class='m-t-md mg-t-10'>" + isread + islike + "<a class='btn btn-xs lbl-default' id='btr_" + msgs.ID + "' data-id=\"" + msgs.isrepl + "\" onclick='btsFun(\"#btr_" + msgs.ID + "\",\"" + msgs.ID + "\",3)'><i class='fa fa-reply'></i> Reply </a><span class='pull-right'>" + isrepo + "</span></div></div></div></div>";
                        $(inst).append(msg);
                    } else {
                        if (userid == msgs.crusid) {
