                                isrepo = "";
                            }
                            msg = "<div id='dld_" + msgs.ID + "'><div class='pull-right col-lg-9'><div class='chat-message'><div class='profile-hdtc'><img class='message-avatar' src='../main/img/contact/" + avatar + " alt=''></div><div class='message' style='background-color:antiquewhite'><a class='message-author'><span style='font-size:12px; color:dodgerblue'> to: </span><span> " + msgs.usernm + " </span></a><span class='message-date' style='font-size:12px; color:gray'> " + msgs.viewdt + " <a style='padding-left:12px' href='#' title='Remove discussion?' onclick='btsDel(\"" + msgs.ID + "\",1)'><i class='fa fa-times-circle-o' style='transform:scale(1.7)'></i></a></span><br><span style='color:dodgerblue; font-size:12px'>" + msgs.L1DS + " > " + msgs.distnm + "  " + acctnm + "</span><span class='message-content'>" + msgs.msgtxt + "</span><div class='m-t-md mg-t-10'><a class='btn btn-xs lbl-default' id='btr_" + msgs.ID + "' data-id=\"" + msgs.isrepl + "\" onclick='btsFun(\"#btr_" + msgs.ID + "\",\"" + msgs.ID + "\",3)'><i class='fa fa-reply'></i> Reply </a><span class='pull-right'>" + isrepo + "</span></div></div></div></div></div>";
                            $("#chatDisc").append(msg);
                        } else {
                            msg = "<div id='dld_" + msgs.ID + "'><div class='pull-left col-lg-10'><div class='chat-message'><div class='profile-hdtc'><img class='message-avatar' src='../main/img/contact/" + avatar + " alt=''></div><div class='message'><a class='message-author'><span style='font-size:12px; color:dodgerblue'> from: </span><span> " + msgs.crusnm + " </span></a><span class='message-date' style='font-size:12px; color:gray'> " + msgs.viewdt + " <a style='padding-left:12px' href='#' title='Remove discussion?' onclick='btsDel(\"" + msgs.ID + "\",1)'><i class='fa fa-times-circle-o' style='transform:scale(1.7)'></i></a></span><br><span style='color:dodgerblue; font-size:12px'>" + msgs.L1DS + " > " + msgs.distnm + "  " + acctnm + "</span><span class='message-content'>" + msgs.msgtxt + "</span><div class='m-t-md mg-t-10'>" + isread + islike + "<a class='btn btn-xs lbl-default' id='btr_" + msgs.ID + "' data-id=\"" + msgs.isrepl + "\" onclick='btsFun(\"#btr_" + msgs.ID + "\",\"" + msgs.ID + "\",3)'><i class='fa fa-reply'></i> Reply </a><span class='pull-right'>" + isrepo + "</span></div></div></div></div></div>";
                            $("#chatDisc").append(msg);
                        }
                    }
                }
            });
            $("#chatDisc").animate({ scrollTop: $('#chatDisc').prop("scrollHeight") }, 1000);
        }
        $(document).ready(function () {
            var mods = @Html.Raw(Json.Encode(@ViewBag.modules));
            $.each(mods, function (ID, mods) {
                $("#selMod").append(
                    $('<option></option>').val(mods.dctpid).html(mods.taskOrder));
            });
            var msgs = @Html.Raw(Json.Encode(@ViewBag.msgs));
            readData(msgs);

        });
        function btsFun(a, b, c) {
            //alert(a); alert(b); alert(c);
            var rd = $(a).attr("data-id");
            var sID = $("#sID").val();
            if (c == 4 && rd == true) {
                return;
            } else if (c == 3 && (sID == null || sID == "")) {
                $("#sID").val(b); 
                $("#textPos").prepend("<div id='replyPos'><label class='col-lg-10' style='color:blue'>> Reply to Message ... " + b + "</label><span class='pull-right'><button class='btn btn-xs btn-danger' onclick='discard()'><i class='fa fa-close'></i><span style='padding-left:8px'>discard</span></button></span></div>");
                $("#txtArea").focus();
            }
            else {
                var txt = [];
                var userid = @ViewBag.userID;
                var tzoffset = (new Date()).getTimezoneOffset() * 60000;
                var dt = (new Date(Date.now() - tzoffset));
                var bt = dt.toISOString(); // ISO "2013-12-08T17:55:38.130Z"
                if (c == 3 && sID != null && sID != "") {
                    var msg = $("#txtArea").val().replace(/(\r\n\t|\n|\r\t)/gm, " ");
                    txt = "<|>" + msg;

                }
                var stra = c + "<|>" + userid + "<|>" + b + "<|>" + bt + txt;
                $.ajax({
                    url: '/uhsCH/messEdit',
                    type: "GET",
                    dataType: "JSON",
                    data: { b: stra },
                    success: function (msgs) {
                        if (msgs == 1) {
                                $(a).remove();
                        } else if (msgs == 2) {
                                if (rd == "false") {
                                    $(a).css("color", "orange").attr("data-id", true);
                                } else if (rd == "true") {
                                    $(a).css("color", "").attr("data-id", false);
                                }
                            }
                        else if (msgs == 4) {
                                $(a).css("color", "orange").attr("data-id", true);
                                $(a).html("").html("<i class='fa fa-bullhorn'></i> Reported!");
                        }
                        else {
                            readData(msgs);
                        }
                        $("#sID").val("");
                        discard();
                    }
                });
            }

        }
        function btsDel(a, b) {
            $.ajax({
                url: '/uhsCH/messDel',
                type: "GET",
                dataType: "JSON",
                data: { a: a },
                success: function (msgs) {
                    if (msgs == 1) {
                        var ms = [];
                        if (b == 1) {
                            ms = "#dld_" + a;
                        } else if (b == 2) {
                            ms = "#dlm_" + a;
                        }
                        $(ms).remove();
                    }
                }
            });
        };
        function discard() {
            $("#replyPos").remove();
            $("#sID").val("");
            $("#selMod").val(0).attr("selected", "selected");
            $("#selDiv").html("");
            $("#selDiv").append(
                $('<option></option>').val(0).html("-"));
            $("#selLoc").html("");
            $("#selLoc").append(
                $('<option></option>').val(0).html("-"));
            $("#selUsr").html("");
            $("#selUsr").append(
                $('<option></option>').val(0).html("-"));
            $("#txtArea").val("");
        };
        $("#commt").on('click', function () {
            var myClass = $(this).attr("class");
            if (myClass == "active") {
                $("#chatDisc").animate({ scrollTop: $('#chatDisc').prop("scrollHeight") }, 1000);
            }
        });
        $("#selMod").on('change', function () {
            var ID = $("#selMod").val();
            $.ajax({
                url: '/uhsCH/getDiv',
                type: "GET",
                dataType: "JSON",
                data: { ID: ID },
                success: function (data) {
                    $("#selDiv").html(""); // clear before appending new list
                    $("#selLoc").html(""); // clear before appending new list
                    $("#selUsr").html(""); // clear before appending new list
                    $("#selDiv").append(
                        $('<option></option>').val(0).html("-"));
                    $.each(data, function (ID, data) {
                        $("#selDiv").append(
                            $('<option></option>').val(data.ID).html(data.Name));
                    });
                    $("#selLoc").append(
                        $('<option></option>').val(0).html("-"));
                    $("#selUsr").append(
                        $('<option></option>').val(0).html("-"));
                }
            });
        });
        $("#selDiv").on('change', function () {
            var ID = $("#selMod").val();
            var IDc = $("#selDiv").val();
            $.ajax({
                url: '/uhsCH/getLoc',
                type: "GET",
                dataType: "JSON",
                data: { ID: ID, IDc: IDc },
                success: function (data) {
                    $("#selLoc").html(""); // clear before appending new list
                    $("#selUsr").html(""); // clear before appending new list
                    $("#selLoc").append(
                        $('<option></option>').val(0).html("-"));
                    $.each(data, function (ID, data) {
                        $("#selLoc").append(
                            $('<option></option>').val(data.ID).html(data.Name));
                    });
                    $("#selUsr").append(
                        $('<option></option>').val(0).html("-"));
                }
            });
        });
        $("#selLoc").on('change', function () {
            var ID = $("#selMod").val();
            var IDb = $("#selLoc").val();
            $.ajax({
                url: '/uhsCH/getUsr',
                type: "GET",
                dataType: "JSON",
                data: { ID: ID, IDb: IDb },
                success: function (data) {
                    $("#selUsr").html(""); // clear before appending new list
                    $("#selUsr").append(
                        $('<option></option>').val(0).html("-"));
                    $.each(data, function (ID, data) {
                        $("#selUsr").append(
                            $('<option></option>').val(data.ID).html(data.Name));
                    });
                }
            });
        });
    </script>
</body>

</html>