    <style>
        footer {
            display: none;
        }

        .buttonb {
            margin: 0 auto;
            font-size: 2.0rem;
            padding: 1.25rem 2.5rem;
            display: block;
            background-color: #ffffff;
            border: 1px solid;
            border-color: #009ac9;
            color: #009ac9;
            font-weight: bold;
            min-height: 100px;
            max-height: 100px;
            min-width: 220px;
            -webkit-border-radius: 3px;
            border-radius: 3px;
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
        }

            .buttonb:hover {
                background-color: #009ac9;
                color: #ffffff;
                border-color: #009ac9;
            }
            .responsiveimg {
    width: 100%;
    height: auto;
}
.startbtn{
    display: none;
}
    </style>
</head><!--/head-->
<body id="home" class="homepage">


    <div class="lckbckscr">

        <div id="wrapper">
            <nav class="navbar navbar-fixed-top" role="navigation" style="background-color:#F5F5F5 !important">
                <div class="navbar-header">
                    <div style="padding-top:4px; padding-left:32px">
                        <a target="_blank" href="https://www.agilitihealth.com"><img src="~/images/logo-header4.png" /></a>
                    </div>
                </div>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse" style="font-size:20px">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
            {
                    @Html.AntiForgeryToken()
                    @Html.Action("_navbartop", "uhsCH")
                }

                <!-- /.navbar-top-links -->
                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        <ul class="nav" id="side-menu">
                            <li class="sidebar-search">
                                <div class="input-group custom-search-form">
                                    <input type="text" class="form-control" placeholder="Search...">
                                    <span class="input-group-btn">
                                        <button class="btn btn-primary" type="button">
                                            <i class="fa fa-search"></i>
                                        </button>
                                    </span>
                                </div>
                                <!-- /input-group -->
                            </li>
                            <li>
                                <a href="/uhsCH/Home"><i class="fa fa-home fa-fw"></i> Home</a>
                            </li>
                            @Html.Action("SBagPr", "uhsCH")
                            @if (ViewBag.userTypeID == 2)
                            {
                                <li style="padding-left:3px">
                                    <a target="_blank" href="/formsCRUD/Index?projectID=1&taskOrderID=1&Int1=1" style="color:#0076A8 !important"><i class="fa fa-table"></i><span style="padding-left:1px"> System Administration</span></a>
                                </li>
                            }
                        </ul>
                    </div>
                </div>
            </nav>

            <div id="page-wrapper">
                <div class="container-fluid">
                    <div class="row">
                        <br />
                        <h3 class="page-header"><span style="color:#0076A8">Application </span><span style="color:#F69240"> Select</span></h3>
                    </div>
                    <div class="row">
                        <div class="startbtn" id="Sdistrict" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhsdistrict/District?projectID=13&taskOrderID=53&Int1=1'" class="buttonb">District<br><span style="color:#F69240">Cadence</span></button>
                        </div>
                        <div class="startbtn" id="Sa360" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhsa360/a360?projectID=13&taskOrderID=55&Int1=1'" class="buttonb">Asset360<br><span style="color:#F69240">Cadence</span></button>
                        </div>
                        <div class="startbtn" id="Sb360" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhsb360/b360?projectID=13&taskOrderID=56&Int1=1'" class="buttonb">BioMed360<br><span style="color:#F69240">Cadence</span></button>
                        </div>
                        <div class="startbtn" id="Scoe" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhscoeops/coecadence?projectID=13&taskOrderID=58&Int1=1'" class="buttonb">COE Operations<br><span style="color:#F69240">Cadence</span></button>
                        </div>
                        <div class="startbtn" id="Ssurserv" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhssurserv/sscadence?projectID=13&taskOrderID=57&Int1=1'" class="buttonb">Surgical Services<br><span style="color:#F69240">Cadence</span></button>
                        </div>
                    </div>
                    <div class="row">
                        <hr />
                    </div>

                    <div class="row">
                        <div class="startbtn" id="Sa3form" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhsworktask/Index?projectID=13&taskOrderID=65&Int1=1'" class="buttonb">A3 Form<br><span style="color:#F69240">Document</span></button>
                        </div>
                        
                    </div>
                    <div class="row">
                        <hr />
                    </div>

                    <div class="row">
                        <div class="startbtn" id="Sstandards" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhsvwpstandard/standards'" class="buttonb">VWP Standards</button>
                        </div>
                        <div class="startbtn" id="SQuality" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhsa360/Quality'" class="buttonb">Quality</button>
                        </div>

                    </div>
                    <div class="row">
                        <hr />
                    </div>
                    <div class="row">
                        <div class="startbtn" id="sscyclecount" style="display: inline-block; padding-bottom:4px">
                            <button onclick="location.href = '/uhssscyccntm/Index?projectID=14&taskOrderID=64&Int1=1'" class="buttonb">Surgical Services<br><span style="color:#F69240">Cycle Count</span></button>
                        </div>

                        <hr />
                    </div>
                    <div class="row" style="color:#F8F8F8; background-color:#F5F5F5; min-height:280px">
                        .
                    </div>
                </div>
            </div>         
        </div>       
    </div>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <!--/# data handler java script-->
    <!--/# data handler java script-->
    <script src="~/js/uhsHome/jquery.js"></script>
    <script src="~/js/uhsHome/bootstrap.min.js"></script>
    <!-- iCheck -->
    <script src="~/vendors/iCheck/icheck.min.js"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/js/metisMenu.min.js"></script>
    <script src="~/js/uhsHome/owl.carousel.min.js"></script>
    <script src="~/js/uhsHome/mousescroll.js"></script>
    <script src="~/js/uhsHome/jquery.prettyPhoto.js"></script>
    <script src="~/js/uhsHome/jquery.isotope.min.js"></script>
    <script src="~/js/uhsHome/jquery.inview.min.js"></script>
    <script src="~/js/uhsHome/wow.min.js"></script>  
    <!-- iCheck -->
    <script src="~/js/metisMenu.min.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/js/startmin.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $(".startbtn").hide();
            var data1 = @Html.Raw(Json.Encode(@ViewBag.preAgentT));
            $.each(data1, function (ID, data1) {
                var it1 = data1.ID;
                if (it1 == 53) {
                    $("#Sdistrict").show();
                    $("#Sstandards").show();
                    //$("#SQuality").show();
                } else if (it1 == 55) {
                    $("#Sa360").show();
                } else if (it1 == 56) {
                    $("#Sb360").show();
                } else if (it1 == 57) {
                    $("#Ssurserv").show();
                } else if (it1 == 58) {
                    $("#Scoe").show();
                } else if (it1 == 64) {
                    $("#sscyclecount").show();
                } else if (it1 == 65) {
                    $("#Sa3form").show();  
                }
            });
        });
    </script>
</body>
</html>