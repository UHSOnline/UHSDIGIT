    <script type="text/javascript">
        $(document).ready(function () {
            $(".startbtn").hide();
            var data1 = @Html.Raw(Json.Encode(@ViewBag.preAgentT));
            $.each(data1, function (ID, data1) {
                var it1 = data1.ID;
                if (it1 == 53) {
                    $("#Sdistrict").show();
                    $("#Sstandards").show();
                    //$("#SQuality").show();
                } else if (it1 == 55) {
                    $("#Sa360").show();
                } else if (it1 == 56) {
                    $("#Sb360").show();
                } else if (it1 == 57) {
                    $("#Ssurserv").show();
                } else if (it1 == 58) {
                    $("#Scoe").show();
                } else if (it1 == 64) {
                    $("#sscyclecount").show();
                } else if (it1 == 65) {
                    $("#Sa3form").show();
                } else if (it1 == 66) {
                    $("#SinvMap").show();
                }
            });
        });
    </script>
