.container .box
{
    position: relative;
    width:200px;
    height:200px;
    background: white;
    transition: 0.5s;
    display: inline-block;
    border: solid 1px #85CDDB;
}
.container .box:hover
{
    transform: scale(1.1);
    z-index: 1;
    box-shadow: 0 2px 10px steelblue;
}

.container .box .thumb
{
    position: absolute;
    width: 100%;
    height:200px;
    overflow: hidden;
}

.container .box .thumb img
{
    width: 100%;
}
.container .box .details
{
    position: absolute;
    width: 100%;
    height:200px;
    overflow: hidden;
    background: white;
}

.container .box .details .content
{
    position: absolute;
    top: calc(50% + 16%);
    transform: translateY(-50%);
    width: 100%;
    padding: 20px;
    box-sizing: border-box;
    text-align: center;
    transition: 0.5s;
}
.container .box:hover .details .content
{
    top: calc(50%);
    background: #F0FFFF;
}

.container .box .details .content .fa
{
    font-size: 80px;
    color: black;
}
.container .box .details .content h3
{
    margin: 0;
    padding: 0;
    padding: 10px 0;
    color: gray;
}
.container .box:hover .details .content h3
{
    color: steelblue;
}
.container .box .details .content a
{
    display: inline-block;
    padding: 5px 20px;
    color: steelblue;
    border: 2px solid black;
    text-decoration: none;
    transition: 0.5s;
    border-radius: 20px;
    transform: scale(0);
}
.container .box:hover .details .content a
{
    transform: scale(1);
}
.container .box .details .content a:hover
{
    background: #fff;
    color: #262626;

}
