            $.each(data1, function (ID, data1) {
                var it1 = data1.ID;
                if(it1 == 53){
                    $("#Sdistrict").show();
                    $("#Sstandards").show();
                    //$("#SQuality").show();
                } else if(it1 == 55){
                    $("#Sa360").show();
                } else if(it1 == 56){
