    <script type="text/javascript">
        function newProject(a) {
            $("head").addClass("loadDiv");
            $('.loadimage').show();
            var url = '/uhsworktask/insertDoc?tskoid=' + a + '&L3IDc=0&dat=@ViewBag.dat&wek=@ViewBag.wek&per=@ViewBag.per&dis=@ViewBag.dis&act=@ViewBag.act&dcg=@ViewBag.dcg&dct=@ViewBag.dct&IDR=null';
            $.ajax({
                url: url,
                type: "GET",
                dataType: "JSON",
                success: function (response) {
                    var urlb = '/uhsworktask/workTask?ID=' + response + '&IDK=' + a + '&IDF=1';
                    window.location = urlb;
                }
            });
        };
        function editProject(a, b) {
            $("head").addClass("loadDiv");
            $('.loadimage').show();
            var urlb = '/uhsworktask/workTask?ID=' + a + '&IDK=' + b + '&IDF=1';
            window.location = urlb;
        };
    </script>
    <script type="text/javascript">
        function delDoc(a) {
            $.confirm({
                title: 'Delete!',
                content: 'Are you sure?',
                buttons: {
                    confirm: function () {
                        $.ajax({
                            url: '/uhsCH/delDoc3010001',
                            type: "GET",
                            dataType: "JSON",
                            data: { id: a },
                            success: function (response) {
                                if (response == 1) {
                                    location.reload();
                                }
                                else {
                                    alert("Error: Failed to delete document!");
                                }
                            }
                        });
                    },
                    cancel: function () {
                    }
