@model List<TRIZMA.Models.UHSWEBAACvDb>

@foreach (var item in Model)
{
    <div class="showdistb col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12" id="@item.ID">
        <div class="panel panel-default">
            <div class="panel-heading">
                <span style="color:#F69240">District: </span><span style="color:#0076A8"> @item.dist</span>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <span style="color:lightsalmon; font-size:smaller">Region: </span><span style="color:#0076A8; font-size:smaller"> @item.region</span><span style="color:white"> .. </span>
                <span style="color:lightsalmon; font-size:smaller">Division: </span><span style="color:#0076A8; font-size:smaller"> @item.divNm</span><span style="color:white"> .. </span>
                <span style="color:lightsalmon; font-size:smaller">DOD: </span><span style="color:#0076A8; font-size:smaller"> @item.DOD</span><span style="color:white"> .. </span>
                <span style="color:lightsalmon; font-size:smaller">OMD: </span><span style="color:#0076A8; font-size:smaller">@item.OMD</span>
                <br />
                <div class="row">
                    <div class="col-lg-4 col-md-8 col-xs-12" style="padding-top:32px">
                        <button onclick="location.href='/uhssurserv/getEditPar1010009?dctp=1010009&id1=@item.ID&id2=1&id3=null'" class="w3-button w3-white w3-border col-lg-12 col-md-12 col-sm-12"><span style="color:#F69240">Surgical Services </span><span style="color:#0076A8; padding-right:12px"> Cadence</span><span style="color:#00966C; padding-top:5px"><i class='fa fa-arrow-circle-right' style='transform:scale(1.2)'></i></span></button>
                        <div class="col-lg-12" align="center">
                            <br />
                            <table class="table-hover table-striped" style="font-size:14px">
                            <tbody>
                                <tr>
                                    @if (item.omdch1 == 0)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">D: </td>
                                        <td style="font-size:smaller; color:red"> Due: @item.dateDT</td>
                                    }
                                    else if (item.omdch1 == 1)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">D: </td>
                                        <td style="font-size:smaller; color:lightseagreen"> Completed!</td>
                                    }
                                </tr>
                                <tr>
                                    @if (item.omdch2 == 0)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">W: </td>
                                        <td style="font-size:smaller; color:red"> Due: @item.dtby1</td>
                                    }
                                    else if (item.omdch2 == 1)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">W: </td>
                                        <td style="font-size:smaller; color:lightseagreen"> Completed!</td>
                                    }
                                </tr>
                                <tr>
                                    @if (item.omdch3 == 0)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">M: </td>
                                        <td style="font-size:smaller; color:red"> Due: @item.dtby2</td>
                                    }
                                    else if (item.omdch3 == 1)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">M: </td>
                                        <td style="font-size:smaller; color:lightseagreen"> Completed!</td>
                                    }
                                </tr>
                                <tr>
                                    @if (item.omdch4 == 0)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">Q: </td>
                                        <td style="font-size:smaller; color:red"> Due: @item.dtby3</td>
                                    }
                                    else if (item.omdch4 == 1)
                                    {
                                        <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                        <td style="font-size:smaller; text-align:right; color:#0076A8; padding-right:5px">Q: </td>
                                        <td style="font-size:smaller; color:lightseagreen"> Completed!</td>
                                    }
                                </tr>
                            </tbody>
                        </table>
                        </div>
                        
                    </div>
                    <div class="col-lg-8 col-md-6 col-xs-12" style="padding-top:14px">
                        <div align="center" style="font-size:11px"><span style="color:#F69240">Surgical Services </span><span style="color:#0076A8"> Cadence Success by Month</span></div>
                        <canvas id="@item.ID AC" height="116"></canvas>
                    </div>
                    
                </div>

            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->

    </div>

}
