    </script>
    <script type="text/javascript">
        function saveModal() {
            $("head").addClass("loadDiv");
            $('.loadimage').show();
        }
        function closeModal() {
            $("head").removeClass("loadDiv");
            $('.loadimage').hide();
        }
    </script>
