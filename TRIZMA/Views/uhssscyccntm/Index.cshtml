    <script type="text/javascript">
        $(document).ready(function () {
            $(".btnNewDoc").on('click', function () {
                var whsid = $("#drop2").val();
                if (whsid == 0 || whsid == null) {
                    alert("Please select District > Tech/Warehouse Location");
                }
                else {
                    saveModal();
                    create1064001();
                }
            });
            function create1064001() {
                var whsid = $("#drop2").val();
                var userID = @ViewBag.userID;
                $.ajax({
                    type: "POST",
                    data: { whsid: whsid, userID: userID },
                    url: '/uhssscyccntm/Create',
                    dataType: "JSON",
                    success: function (ddID) {
                        if (ddID == 1) {
                            location.reload();
                        }
                        else {
                            alert("Error: Failed to create new document!");
                        }
                    }
                });
            };
        });
    </script>
    <script type="text/javascript">
        function delFunct(a) {
            $.confirm({
                title: 'Delete!',
                content: 'Are you sure?',
                buttons: {
                    confirm: function () {
                        deleteFiles(a);
                    },
                    cancel: function () {
                    }
                }
            });
        }
        function deleteFiles(a) {
            $.ajax({
                type: "POST",
                data: { IDc: a },
                url: '/uhssscyccntm/Delete',
                dataType: "JSON",
                success: function () {
                    location.reload();
                },
                error: function (data) {
                    alert("Failed to Delete document!");
                }
            });
        }
        function exportDoc(a) {
                var idr = "#" + a;
                var chk = $(idr).is(":checked");
                if (chk == 0) {
                    alert("This document is UnLocked. Only Locked documents will be exported!");
                } else {
                    saveModal();
                    $.ajax({
                        type: "POST",
                        data: { IDc: a },
                        url: '/uhssscyccntm/exportDoc',
                        dataType: "JSON",
                        success: function (ddID) {
                            alert("Document export Successful!");
                            closeModal();
                        },
                        error: function (ddID) {
                            alert("Failed to Export document!");
                            closeModal();
                        }
                    });
                }
            }
            function lockDoc(a) {
                saveModal();
                var b = "#" + a;
                var checked = $(b).is(":checked");
                if (checked == true) {
                    var c = 1;
                    $.confirm({
                        title: 'This document will be locked!',
                        content: 'All products in this document will be locked for editing. Only users with admin role will be able to unlock it. Do you want to continue?  ',
                        buttons: {
                            confirm: function () {
                                lockDocument(a, b, c);
                            },
                            cancel: function () {
                                $(b).prop("checked", false);
                                closeModal();
                            }
                        }
                    });

                }
                else {
                    var c = 0;
                    lockDocument(a, b, c);
                }

            }
            function lockDocument(a,b,c) {
                $.ajax({
                    type: "POST",
                    data: { IDc: a, Int1: c },
                    url: '/uhssscyccntm/lockDoc',
                    dataType: "JSON",
                    success: function (ddID) {
                        var user = @ViewBag.userTypeb;
                        if (c == 1 && user > 2) {
                            $(b).prop("disabled", true);
                        }
                        location.reload();
                    },
                    error: function (data) {
                        $(b).prop("checked", false);
                        alert("Failed to lock document");
                        closeModal();
                    }
                });
            }
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('.loadimage').hide();
