@model List<TRIZMA.Models.UHSWEBAABvDb>

@foreach (var item in Model)
{
<div class="showdist col-xl-6 col-lg-6 col-md-12" id="@item.ID">
        <div class="panel panel-default">
            <div class="panel-heading">
                <span style="color:#F69240"><b>District: </b></span><span style="color:#0076A8"><b> @item.dist</b></span>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <span style="color:lightsalmon; font-size:smaller">Region: </span><span style="color:#0076A8; font-size:smaller"> @item.region</span><span style="color:white"> .. </span>
                <span style="color:lightsalmon; font-size:smaller">Division: </span><span style="color:#0076A8; font-size:smaller"> @item.divNm</span><span style="color:white"> .. </span>
                <span style="color:lightsalmon; font-size:smaller">DOD: </span><span style="color:#0076A8; font-size:smaller"> @item.DOD</span><span style="color:white"> .. </span>
                <span style="color:lightsalmon; font-size:smaller">OMD: </span><span style="color:#0076A8; font-size:smaller">@item.OMD</span>
                <br /><br />
                <div class="row">
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#@item.TAB01" data-toggle="tab">District Cadence</a>
                        </li>
                        <li>
                            <a href="#@item.TAB02" data-toggle="tab">GembaWalk & 6S List</a>
                        </li>
                        <li>
                            <a href="#@item.TAB03" data-toggle="tab">VWP Assessment</a>
                        </li>
                        <li>
                            <a href="#@item.TAB04" data-toggle="tab">Overall Success</a>
                        </li>
                    </ul>
                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div class="tab-pane fade in active" id="@item.TAB01">
                            <br />
                            <div class="col-lg-4 col-md-12 col-xs-12" style="padding-top:24px">
                                <button onclick="location.href='/uhsdistrict/getEditPar1010008?dctp=1010008&id1=@item.ID&id2=1&id3=null'" class="w3-button w3-white w3-border col-lg-12 col-md-12 col-sm-12"><span style="color:#F69240">District </span><span style="color:#0076A8; padding-right:12px"> Cadence</span><span style="color:#00966C; padding-top:5px"><i class='fa fa-arrow-circle-right' style='transform:scale(1.2)'></i></span></button>
                                <br>
                                <div align="center" style="padding-top:20px">
                                    <br />
                                    <table class="table-hover table-responsive">
                                        <tbody>
                                            <tr>
                                                @if (item.omdch1 == 0)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-close" style="color:lightpink; padding-left:2px"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">D: </td>
                                                    <td style="color:red"> Due: Today!</td>
                                                }
                                                else if (item.omdch1 == 1)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">D: </td>
                                                    <td style="color:lightseagreen"> Completed!</td>
                                                }
                                            </tr>
                                            <tr>
                                                @if (item.omdch2 == 0)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-close" style="color:lightpink; padding-left:2px"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">W: </td>
                                                    <td style="color:red"> Due: @item.dtby3</td>
                                                }
                                                else if (item.omdch2 == 1)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">W: </td>
                                                    <td style="color:lightseagreen"> Completed!</td>
                                                }
                                            </tr>
                                            <tr>
                                                @if (item.omdch3 == 0)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-close" style="color:lightpink; padding-left:2px"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">M: </td>
                                                    <td style="color:red"> Due: @item.dtby4</td>
                                                }
                                                else if (item.omdch3 == 1)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">M: </td>
                                                    <td style="color:lightseagreen"> Completed!</td>
                                                }
                                            </tr>
                                            <tr>
                                                @if (item.omdch4 == 0)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-close" style="color:lightpink; padding-left:2px"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">Q: </td>
                                                    <td style="color:red"> Due: @item.dtby5</td>
                                                }
                                                else if (item.omdch4 == 1)
                                                {
                                                    <td style="padding-right:5px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                                    <td style="padding-right:5px; color:#0076A8">Q: </td>
                                                    <td style="color:lightseagreen"> Completed!</td>
                                                }
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                            <div class="col-lg-8 col-md-12 col-xs-12">
                                <div align="center" style="font-size:small"><span style="color:#0076A8">District Cadence </span><span style="color:darkseagreen"> Success Graph</span></div>
                                <canvas id="@item.ID AC" height="116"></canvas>
                            </div>

                        </div>
                        <div class="tab-pane fade" id="@item.TAB02">
                            <br />
                            <div class="col-lg-4 col-md-12 col-xs-12" style="padding-top:24px">
                                <button onclick="location.href='/uhsgw6s/getCrPar1010001?distid=@item.ID&acctid=0&actype=3&docid=null'" class="w3-button w3-white w3-border col-lg-12 col-md-12 col-sm-12"><span style="color:#0076A8; padding-right:12px"> Gemba Walk & 6S List</span><span style="color:#00966C; padding-top:5px"><i class='fa fa-arrow-circle-right' style='transform:scale(1.2)'></i></span></button>
                                <br><br /><br />
                                <div align="center">
                                    <table class="table-hover table-responsive" style="font-size:14px">
                                    <thead>
                                        <tr style="font-size:smaller">
                                            <th>Task:</th>
                                            <th style="padding-right:6px">D:</th>
                                            <th style="padding-right:6px">W:</th>
                                            <th style="padding-right:9px">Due:</th>   
                                            <th>A:</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td style="padding-right:6px; color:#0076A8">Gemba: </td>
                                            @if (item.GWDAY == 0)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                            }
                                            else if (item.GWDAY == 1)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                            }

                                            @if (item.GWWEE == 0)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                                <td style="color:red; padding-right:9px">@item.dtby1</td>
                                            }
                                            else if (item.GWWEE == 1)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                                <td style="color:lightseagreen; padding-right:9px">Completed</td>
                                            }
                                            
                                            @if (item.GWAPR == false)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                            }
                                            else if (item.GWAPR == true)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                            }
                                        </tr>
                                        <tr>
                                            <td style="padding-right:6px; color:#0076A8">6S List: </td>
                                            @if (item.S6DAY == 0)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                            }
                                            else if (item.S6DAY == 1)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                            }

                                            @if (item.S6WEE == 0)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                                <td style="color:red; padding-right:9px">@item.dtby2</td>
                                            }
                                            else if (item.S6WEE == 1)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                                <td style="color:lightseagreen; padding-right:9px">Completed</td>
                                            }
                                            
                                            @if (item.S6APR == false)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-close" style="color:lightpink"></i></td>
                                            }
                                            else if (item.S6APR == true)
                                            {
                                                <td style="padding-right:6px"><i class="fa fa-check" style="color:lightseagreen"></i></td>
                                            }
                                        </tr>

                                    </tbody>
                                </table>
                                </div>
                                
                            </div>
                            <div class="col-lg-8 col-md-12 col-xs-12">
                                <div align="center" style="font-size:small"><span style="color:#0076A8">GembaWalk & 6S List </span><span style="color:darkseagreen"> Success Graph</span></div>
                                <canvas id="@item.ID BC" height="116"></canvas>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="@item.TAB03">
                            <br />
                            <div class="col-lg-4 col-md-12 col-xs-12" align="center" style="padding-top:26px">
                                <button onclick="location.href='/uhsvwpassessment/getCrPar1010003?distid=@item.ID&acctid=0&actype=3&docid=null'" class="w3-button w3-white w3-border col-lg-12 col-md-12 col-sm-12"><span style="color:#0076A8; padding-right:12px"> VWP Assessment</span><span style="color:#00966C; padding-top:5px"><i class='fa fa-arrow-circle-right' style='transform:scale(1.2)'></i></span></button>
                                <div class="col-lg-6 col-md-12 col-xs-12">
                                    <div style="padding-top:16px">
                                        <span style="color:#367EAD; font-size:smaller">Score: </span>
                                    </div>
                                    <div>
                                        <span style="font-size:36px; color:#0076A8"><b>@item.AASTO</b></span>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-12 col-xs-12">
                                    <div style="padding-top:16px">
                                        <span style="color:#367EAD; font-size:smaller">Percent </span><span style="color:#F69240; font-size:smaller"> %</span><span style="color:#367EAD; font-size:smaller">:</span>
                                    </div>
                                    <div>
                                        <span style="font-size:36px; color:#F69240"><b>@item.AASPC</b></span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-8 col-md-12 col-xs-12" style="padding-top:6px">
                                <table id="@item.TB01" class="table-responsive table-hover table-striped" style="font-size:14px; min-width:100%">
                                    <thead>
                                        <tr>
                                            <th style="font-size:12px; color:#005F86">Area:</th>
                                            <th style="text-align:right; font-size:12px; color:#005F86">Score:</th>
                                            <th style="text-align:right; font-size:12px; color:#005F86">Max:</th>
                                            <th style="text-align:right; font-size:12px; color:#005F86">Percent:</th>
                                            <th style="text-align:right; font-size:12px; color:#005F86">Overall%:</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <br />
                            </div>
                            <br />
                        </div>
                        <div class="tab-pane fade" id="@item.TAB04">
                            <br />
                            <div class="col-lg-12 col-md-12 col-xs-12">
                                <div align="center" style="font-size:small"><span style="color:#0076A8">District Overall </span><span style="color:darkseagreen"> Success Graph</span></div>
                                <div>
                                    <canvas id="@item.ID TT" height="76"></canvas>
                                </div>
                                
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>

    
}
